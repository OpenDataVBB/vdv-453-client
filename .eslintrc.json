{
	"extends": "eslint:recommended",
	"env": {
		// "commonjs": true,
		"es6": true,
		"es2016": true,
		"es2017": true,
		"es2018": true,
		"node": true
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"ignorePatterns": [
		"node_modules"
	],
	"rules": {
		"no-unused-vars": "warn"
	}
}
